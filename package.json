{
  "name": "krosmobot",
  "version": "0.1.0",
  "private": false,
  "description": "A stupid Discord bot for a stupid Dofus guild...",
  "repository": "https://github.com/brinkflew/krosmobot.git",
  "author": "Antoine van Serveyt <avs+krosmobot@avanserv.com>",
  "license": "MIT",
  "main": "dist/client.js",
  "engines": {
    "node": "v14.15.0"
  },
  "scripts": {
    "lint": "eslint --ext ts src",
    "lint:fix": "eslint --ext ts src --fix",
    "clean": "rm -rf dist",
    "build": "tsc -p .",
    "build:clean": "yarn clean && yarn build",
    "start": "node dist/client.js",
    "postinstall": "yarn build",
    "precommit": "yarn lint",
    "serve": "nodemon -e ts --exec 'yarn lint && yarn build:clean && node dist/client.js'"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "mongoose": "^5.10.15",
    "nanoid": "^3.1.18",
    "rss-parser": "^3.9.0"
  },
  "devDependencies": {
    "@skyra/eslint-config": "^5.3.3",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.22",
    "@types/common-tags": "^1.8.0",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.9",
    "@types/ws": "^7.4.0",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
